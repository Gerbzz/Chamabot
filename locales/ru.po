msgid ""
msgstr ""
"Content-Type: text/plain; charset=UTF-8\n"

msgid "_lang"
msgstr "ru"

msgid "Error"
msgstr "Ошибка"

msgid "no players"
msgstr "нет игроков"

msgid "{match_id}{queue} is started!"
msgstr "{match_id}{queue} начался!"

msgid "Reverting {queue} to the gathering stage..."
msgstr "Возвращаем {queue} на стадию сбора..."

msgid "Maps"
msgstr "Карты"

msgid "__**{queue}** is now on the check-in stage!__"
msgstr "__**{queue}** на стадии потверждения участников!__"

msgid "Please react with {ready_emoji} to **check-in** or {not_ready_emoji} to **abort**!"
msgstr "Нажмите {ready_emoji} чтобы **подтвердить** или {not_ready_emoji} чтобы **отменить** участие!"

msgid "Waiting on: {players}."
msgstr "Ожидаем: {players}."

msgid "{members} was not ready in time."
msgstr "Не подтвердили участие: {members}."

msgid "{member} has aborted the check-in."
msgstr "{member} отменил свое участие."

msgid "The match is not on the check-in stage."
msgstr "Матч не на стадии подтверждения участников."

msgid "You are not in an active match."
msgstr "Вы не в матче."

msgid "{queue} is now on the draft stage!"
msgstr "{queue} на стадии драфта!"

msgid "VERSUS"
msgstr "ПРОТИВ"

msgid "Unpicked"
msgstr "Кандидаты"

msgid "Type {cmd} to become a captain and start picking teams."
msgstr "Используйте {cmd} чтобы стать капитаном и начать выбирать игроков."

msgid "{member}'s turn to pick!"
msgstr "Черед выбирать {member}!"

msgid "The match is not on the draft stage."
msgstr "Матч не на стадии драфта."

msgid "You must possess the captain's role."
msgstr "У вас должна быть роль капитана."

msgid "Team with name '{name}' not found."
msgstr "Команда '{name'} не найдена."

msgid "Team {name} already have a captain."
msgstr "В команде {name} уже есть капитан."

msgid "You are not a captain."
msgstr "Вы не капитан команды."

msgid "Not your turn to pick."
msgstr "Не ваш черёд выбирать игрока."

msgid "Specified player not in the unpicked list."
msgstr "Указанный игрок не в списке кандидатов."

msgid "The match must be on the draft or waiting report stage."
msgstr "Матч должен быть на стадии драфта или ожидании результата."

msgid "Specified player is not looking for a substitute."
msgstr "Указаный игрок не ищет замену."
